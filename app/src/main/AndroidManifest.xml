<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tommy.sqldemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.KickWaxActivity"
            android:label="@string/title_activity_kick_wax" >
        </activity>
        <activity
            android:name=".Activities.NewEditKickWax"
            android:label="@string/title_activity_new_edit_kick_wax" >
        </activity>
        <activity
            android:name=".Activities.GlidewaxActivity"
            android:label="@string/title_activity_glidewax"
            android:parentActivityName=".Activities.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tommy.sqldemo.Activities.MainActivity" />
        </activity>
    </application>

</manifest>
